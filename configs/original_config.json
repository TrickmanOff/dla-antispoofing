{
  "name": "test",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000
  },
  "arch": {
    "type": "RawNet2",
    "args": {
      "sinc_conv_config": {
        "min_low_hz": 50,
        "min_band_hz": 50,
        "freqs_partition_mode": "s1",
        "kernel_size": 129,
        "abs_output": true
      },
      "res_blocks_out_channels": [128, 128, 512, 512, 512, 512]
    }
  },
  "loss": {
    "type": "CrossEntropyLoss",
    "args": {
      "spoofed_weight": 1.0,
      "bonafide_weight": 9.0
    }
  }
}
