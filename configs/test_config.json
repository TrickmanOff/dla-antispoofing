{
  "name": "test",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000
  },
  "arch": {
    "type": "RawNet2",
    "args": {
      "sinc_conv_config": {
        "min_low_hz": 0,
        "min_band_hz": 0,
        "freqs_partition_mode": "s1",
        "kernel_size": 1024,
        "abs_output": true
      },
      "res_blocks_out_channels": [20, 20, 128, 128, 128, 128],
      "normalize_before_gru": true,
      "gru_config": {
        "num_layers": 3
      }
    }
  },
  "loss": {
    "type": "CrossEntropyLoss",
    "args": {
      "spoofed_weight": 1.0,
      "bonafide_weight": 9.0
    }
  }
}
